<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>HolaMundo</servlet-name>
        <servlet-class>com.iesvdc.acceso.controlador.HolaMundo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlumnoCreate</servlet-name>
        <servlet-class>com.iesvdc.acceso.controlador.AlumnoCreate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Alumno</servlet-name>
        <servlet-class>com.iesvdc.acceso.controlador.Alumno</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlumnoDelete</servlet-name>
        <servlet-class>com.iesvdc.acceso.controlador.AlumnoDelete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlumnoUpdate</servlet-name>
        <servlet-class>com.iesvdc.acceso.controlador.AlumnoUpdate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HolaMundo</servlet-name>
        <url-pattern>/HolaMundo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlumnoCreate</servlet-name>
        <url-pattern>/AlumnoCreate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Alumno</servlet-name>
        <url-pattern>/Alumno</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlumnoDelete</servlet-name>
        <url-pattern>/AlumnoDelete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlumnoUpdate</servlet-name>
        <url-pattern>/AlumnoUpdate</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
